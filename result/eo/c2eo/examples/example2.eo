+package c2eo.examples.example2

+alias c2eo.stdio.printf
+alias c2eo.coperators.add
+alias c2eo.system.ram

[arg...] > global

  ram 1024 > g-ram

  [arg...] > main
    seq > @
      g-ram.write 0 (8.as-bytes)
      g-ram.write 8 (16.as-bytes)
      printf "8 + 16 = %d\n" (add ((g-ram.read 0 8).as-int) ((g-ram.read 8 8).as-int))
      TRUE

  main arg > @
