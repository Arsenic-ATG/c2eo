+package c2eo.examples

+alias org.eolang.io.stdout
+alias org.eolang.txt.sprintf

+alias c2eo.ctypes.intC

[args] > simpleReturnC

  [a b] > f

    intC > c

    if. > @
      seq
        c.write (a.add b)
      c
      error "Unexpected behavior"

  [] > main
    intC > a
    intC > result

    seq > @
      a.write (^.args.get 0).toInt
      result.write (^.f a a)
      stdout (sprintf "simpleReturn[%d] = %d \n" a result)
